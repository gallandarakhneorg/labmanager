create table journal_papers (number varchar(255), pages varchar(255), series varchar(255), volume varchar(255), id integer not null, journal_id integer, primary key (id));
create table journals (id integer not null, address varchar(255), isbn varchar(255), issn varchar(255), journal_name varchar(255), journal_url varchar(32672), open_access boolean, publisher varchar(255), scimago_id varchar(255), wos_id varchar(255), primary key (id));
