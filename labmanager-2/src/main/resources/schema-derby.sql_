create table jury_memberships_promoters (jury_membership_id integer not null, promoters_id integer not null);
create table key_notes (address varchar(255), editors varchar(32672), organization varchar(255), scientific_event_name varchar(32672), id integer not null, primary key (id));
create table memberships (id integer not null, cnu_section varchar(255), conrs_section varchar(255), french_bap varchar(255), is_main_position boolean not null, member_since_when date, member_status varchar(255), member_to_when date, responsibility varchar(255), organization_address_id integer, person_id integer, research_organization_id integer, primary key (id));
