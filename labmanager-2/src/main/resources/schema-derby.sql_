create table org_addresses (id integer not null, city varchar(255) not null, complement varchar(32672), map_coordinates varchar(255), name varchar(255) not null, street varchar(32672) not null, zip_code varchar(255), primary key (id));
create table patents (address varchar(255), institution varchar(255), patent_number varchar(255), patent_type varchar(255), id integer not null, primary key (id));
create table persons (id integer not null, academiaurl varchar(255), cordisurl varchar(255), dblpurl varchar(255), email varchar(255), facebook_id varchar(255), first_name varchar(255), gender varchar(255), github_id varchar(255), google_scholar_hindex integer, google_scholar_id varchar(255), gravatar_id varchar(255), last_name varchar(255), linked_in_id varchar(255), mobile_phone varchar(32), office_phone varchar(32), office_room varchar(255), orcid varchar(255), research_gate_id varchar(255), researcher_id varchar(255), web_page_id varchar(255), web_page_naming varchar(255), wos_hindex integer, primary key (id));
create table publications (id integer not null, abstract_text clob(32672), dblp_url varchar(32672), doi varchar(255), extra_url varchar(32672), hal_id varchar(255), isbn varchar(255), issn varchar(255), keywords clob(32672), major_language varchar(255), manual_validation_forced boolean not null, path_to_downloadable_award_certificate varchar(32672), path_to_downloadablepdf varchar(32672), publication_date date, publication_year integer, title varchar(32672), type varchar(255), video_url varchar(32672), primary key (id));
create table reports (address varchar(255), institution varchar(255), report_number varchar(255), report_type varchar(255), id integer not null, primary key (id));
create table research_orgs (id integer not null, acronym varchar(255), country varchar(255) not null, description clob(32672), name varchar(32672), organization_url varchar(32672), type varchar(255) not null, super_organization_id integer, primary key (id));
create table research_orgs_addresses (research_organization_id integer not null, addresses_id integer not null, primary key (research_organization_id, addresses_id));
create table theses (address varchar(255), institution varchar(255), id integer not null, primary key (id));
