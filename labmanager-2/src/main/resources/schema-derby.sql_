create table authorship (id integer not null, author_rank integer not null, person_id integer, publication_id integer, primary key (id));
create table book_chapters (address varchar(255), book_title varchar(32672), chapter_number varchar(255), edition varchar(255), editors varchar(32672), number varchar(255), pages varchar(255), publisher varchar(255), series varchar(255), volume varchar(255), id integer not null, primary key (id));
create table books (address varchar(255), edition varchar(255), editors varchar(32672), number varchar(255), pages varchar(255), publisher varchar(255), series varchar(255), volume varchar(255), id integer not null, primary key (id));
create table conference_papers (address varchar(255), editors varchar(32672), number varchar(255), organization varchar(255), pages varchar(255), publisher varchar(255), scientific_event_name varchar(255), series varchar(255), volume varchar(255), id integer not null, primary key (id));
